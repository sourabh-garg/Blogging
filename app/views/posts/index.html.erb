

<h1 id="page-title">Stuff I've written</h1>
	<div id="posts-wrapper" class="skinny_wrapper">

		<% @post.each do |post|%>
	<div class="post">
		<p class="date"><%= post.created_at.strftime('%A, %B %d') %></p>
		<h2><%= post.title%></h2>
		<hr>
	</div>
	<% end %>
	</div>

	<%= will_paginate @post%>

	